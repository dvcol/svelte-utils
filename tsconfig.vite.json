{
  "extends": ["@tsconfig/node22/tsconfig.json", "./tsconfig.json"],
  "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.vite.tsbuildinfo",
  "exclude": [],
  "include": [
    "**/*.ts",
    "**/*.svelte",
    "**/*.spec.ts",
    "**/*.test.ts",
    "*.config.*",
    "svelte.config.js",
    ".eslintrc.cjs",
    "README*md/*.ts",
    "README*md/*.svelte",
    "package.json"
  ],
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "strict": true,
    "noEmit": true,
    "types": ["node", "vitest"]
  }
}
